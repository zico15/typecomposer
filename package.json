{
  "name": "typecomposer",
  "version": "0.0.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/zico15"
  },
  "description": "A component library for getting people started with easy to re-use components for everyday projects.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "styles",
    "typings.d.ts",
    "public"
  ],
  "scripts": {
    "rm-styles": "rm -rf ./styles && cp -r ./src/assets/styles .",
    "copy": "cp -r dist /Users/ezequiel/Documents/TypeCompose/vite-project/node_modules/typecomposer",
    "build": "npm run rm-styles && rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs && npm run copy",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "pub": "npm run build && npm publish"
  },
  "keywords": [
    "typecomposer"
  ],
  "preferGlobal": true,
  "author": "The Nerdy Canuck",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.0",
    "ts-morph": "^20.0.0",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "ts-morph": "^20.0.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "@googlemaps/js-api-loader": "^1.16.2"
  }
}
